        <module>springboot-31-webflux</module>
        <module>springboot-32-jvm</module>
        <module>springboot-33-JMH</module>
        <module>springboot-34-markdown</module>
        <module>springboot-35-nutz</module>
        <module>springboot-36-elastic-job-lite</module>
        <module>springboot-36-elastic-job-lite-1</module>
        <module>springboot-37-xxl-job</module>
        <module>springboot-38-Sharding-JDBC</module>
        <module>springboot-39-netty-socketio</module>
        <module>springboot-40-hystrix</module>
        <module>springboot-41-mybatis-plus</module>
        <module>springboot-42-tomcat</module>
